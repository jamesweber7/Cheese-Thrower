{
    "manifest_version": 3,
    "name": "Cheese Thrower",
    "version": "1.0.0",
    "description": "Throw Cheese",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/cheese16.png",
            "48": "assets/cheese48.png",
            "128": "assets/cheese128.png"
        }
    },
    "web_accessible_resources": [
        {
            "resources": [ "assets/cheese.png", "assets/cheese_smack*.wav" ],
            "matches": [ "<all_urls>" ]
        }
    ],
    "icons": {
        "16": "assets/cheese16.png",
        "48": "assets/cheese48.png",
        "128": "assets/cheese128.png"
    }
}
